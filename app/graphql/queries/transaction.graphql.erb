query transaction($base_path: String!, $locale: String!) {
  edition(base_path: $base_path, locale: $locale) {
    ...DefaultTopLevelFields
    details {
      department_analytics_profile
      external_related_links
      introductory_paragraph
      more_information
      start_button_text
      transaction_start_link
      variants
      will_continue_on
    }
    links {
      ...AvailableTranslations
      ...DocumentCollections
      ...MainstreamBrowsePages
      ...Organisations
      ...Parents
      ...PrimaryPublishingOrganisation
      ...SuggestedOrderedRelatedItems
      ...Taxons
    }
  }
}
